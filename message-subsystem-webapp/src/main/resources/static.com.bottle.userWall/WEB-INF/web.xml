<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>DoPost</servlet-name>
    <servlet-class>com.bottle.userWall.utils.DoPost</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Show</servlet-name>
    <servlet-class>com.bottle.userWall.utils.Show</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DeleteAll</servlet-name>
    <servlet-class>com.bottle.userWall.utils.DeleteAll</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ShowLast3</servlet-name>
    <servlet-class>com.bottle.userWall.utils.ShowLast3</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Show</servlet-name>
    <url-pattern>/show</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DoPost</servlet-name>
    <url-pattern>/doPost</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DeleteAll</servlet-name>
    <url-pattern>/deleteAll</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ShowLast3</servlet-name>
    <url-pattern>/showLast3</url-pattern>
  </servlet-mapping>


</web-app>
